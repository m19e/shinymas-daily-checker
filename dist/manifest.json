{
    "name": "daily-checker",
    "description": "Open mission page when update daily mission",
    "version": "0.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["index.js"]
        }
    ],
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    }
}
